package codingExam;

public class CoinExam {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int n = 1260;
		int[] coin = {500,100,50,10};
		int count = 0;
		for(int i=0;i<coin.length;i++) {
			//몫
			int ahrt = n%coin[i];
			//코인갯수
			count += n/coin[i];
			//n값 초기화
			n = ahrt;
			
		}
		
		System.out.println("코인의 갯수는 "+count+"개 입니다.");
		
	}

}
